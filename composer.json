{
  "name": "pixels-starter-theme",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "WordPress starter theme mixing Timber Twig templates and webpack.",
  "homepage": "https://github.com/pixelshelsinki/pixels-starter-theme",
  "authors": [
    {
      "name": "Thomas Hurd",
      "email": "thomas@pixels.fi",
      "homepage": "https://github.com/seennothurd"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/pixelshelsinki/pixels-starter-theme/issues"
  },
  "autoload": {
    "psr-4": {
      "Roots\\Sage\\": "lib/Sage/"
    }
  },
  "require": {
    "php": ">=5.6.4",
    "composer/installers": "~1.0",
    "illuminate/view": "~5.4.0",
    "illuminate/config": "~5.4.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.8.0"
  },
  "scripts": {
    "test": [
      "phpcs"
    ],
    "post-create-project-cmd": [
        "Roots\\Sage\\PostCreateProject::updateHeaders",
        "Roots\\Sage\\PostCreateProject::selectFramework",
        "Roots\\Sage\\PostCreateProject::addFontAwesome",
        "Roots\\Sage\\PostCreateProject::buildOptions"
    ]
  }
}
